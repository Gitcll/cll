	public String toString() {
		return "EmpAndKeshi [e_id=" + e_id + ", username=" + username
				+ ", userlogin=" + userlogin + ", userpwd=" + userpwd
				+ ", hiredate=" + hiredate + ", sex=" + sex + ", age=" + age
				+ ", education=" + education + ", addr=" + addr + ", iphone="
				+ iphone + ", q_id=" + q_id + ", usertype=" + usertype
				+ ", pos=" + pos + ", k_id=" + k_id + ", k_name=" + k_name
				+ ", d_id=" + d_id + ", d_name=" + d_name + "]";
