/*default*/
body,h1,h2,h3,h4,h5,h6,p,span,strong,ul,li,ol{
	padding: 0;
	margin: 0;
}
body{
	font-size: 12px;
	font-family: "微软雅黑";
}
li{
	list-style: none;
}
img{
	vertical-align: top;
	border: none;
}
a{
	text-decoration: none;
	color: #000;
}
.clear{clear:both;}
/*end default*/

/*head*/
.head{
	width:100%;
	margin: 0 auto;
}
.head_top{
	width:960px;
	height: 110px;
	margin: 0 auto;
}
.head_top h1{
	display: inline;
}
.head_top h1 img{
	width: 450px;
	height: 120px;
	background: red;
}
.head_top_search{
	float: right;
	width: 261px;
	height: 60px;
	margin-top: 40px;
}
.text{
	width: 200px;
	height: 26px;
	border: none;
	background: #eee;
	text-indent: 1em;
	color: #999999;
	position: relative;
}
.btn{
	position: absolute;
	
	width: 60px;
	height: 28px;
	border: none;
	background: #6dd1e0;
	color: #fff;	
}
.head_top_search span{
	font-size: 16px;
	color: #6dd1e0
}
.head_top_search a{
	font-size: 16px;
	color: #999;
}
.head_top_search a:hover{
	color: #6dd1e0
}
/*nav*/
.header_nav{
	width: 100%;
	background: #0aa6bd;
	margin-top: 10px;
	
}

.header_nav .nav_F{
	height: 50px;
	width: 960px;
	margin: 0 auto;
}
.nav_F li{
	width: 90px;
	float: left;
	font-size:18px;
	font-weight: normal;
	text-align: center;
	line-height: 50px;
	margin-right: 14px;
}
.nav_F li a{
	color: #fff; 
}
.nav_F li:hover ul{
	display: block;
}
.nav_F li a:hover{
	color: lightskyblue	
}
.nav_F li ul{	
	display: none;
}
.nav_F li ul li{
	background: rgba(255,255,255,0.9);
	width: 95px;
	height: 40px;
	font-size: 14px;
	text-align:  center;
	line-height: 40px;
	padding-left: 2px;
	border-bottom: 1px solid rgba(96,96,96,0.5);
	margin-top: 1px;
	border-radius: 5px;
}
.nav_F li ul li:hover{
	background: rgba(241,241,241);
}
.nav_F li ul li a{
	color: #000;
}
/*end nav*/
/*头部图片*/
.head_img{
	position: absolute;
	z-index: -1;
	width: 100%;
	height: 320px;
	background: red;
}
 .head_bg{
	height: 320px;
}
.head_img ul{
	width: 300px;
	height: 30px;
	margin: 0 auto;
	margin-top: 10px;
}
.head_img ul li{
	float: left;
	width: 36px;
	height: 15;
	margin-right: 6px;
	height: 12px;
	background: #0da5bc;
}
.head_img ul li:hover{
	background: orange;
}
/*end 头部图片*/
/*end head*/
/*内容区域*/
/*内容上面区域*/
.contert{
	width: 960px;
	margin: 350px auto 0;
}
.contert_ul li{
	position: relative;
	float: left;
	width: 155px;
	height: 100px;
	background: teal;
	margin-right: 5px;
	line-height: 30px;
}
.contert_ul li img{
	position: absolute;
	top:20px;
	left:55px;
}
.contert_ul li span{
	position: absolute;
	bottom: 10px;
	left: 55px;
	color:#000;
}
/*内容中间区域left*/
.contert_center{
	width: 960px;
	height: 200px;
	position: relative;
	top: 10px;
	margin-bottom: 50px;
}
.center_left{
	float: left;
	margin-right: 20px;
	width: 300px ;
	height: 240px;
	position: relative;
}
.center_left img{
	width: 300px ;
	height: 200px;
}
.center_left span{
	width: 300px;
	height: 30px;
	display: block;
	background: rgba(0,0,0,0.5);
	color: #FFFFFF;
	font-size: 13px;
	line-height: 30px;
	font-weight: bold;
	text-indent: 1em;
}
.center_left ul{
	width: 70px;
	height: 30px;
	margin-top: 10px;
	position: absolute;
	bottom: 2px;
	right: 0px;
}
.center_left ul li {
	float: left;
	width: 20px;
	height: 15;
	margin-right: 2px;
	height: 12px;
	background: #0da5bc;
}
.center_left ul li:hover{
	background: #FFFFFF;
}
/*内容中间区域center*/
.center_news{
	width: 300px;
	height: 230px;
	float: left;
}
.center_news span,.bottom span{
	background: #0aa6bd;
	font-weight: bold;
	font-size: 14px;
	padding: 2px 10px;
	color: #FFFFFF;
}
.center_news .more{
	float: right;
	color: #ccc;
	font-size: 12px;
}
.center_news ul{
	width: 300px;
	margin-top: 6px;
}
.center_news ul li{
	font-size: 13px;
	line-height: 30px;
}
.center_news ul li a:hover{
	color: #CCCCCC;
}
.center_right{
float: right
}

/*内容下面区域*/
.bottom{
	position: relative;
}
.bottom_left{
	width: 640px;
}
.dept{
	width: 640px;
	height: 100px;
	margin: 5px 0;	
}
.dept_left{
	width: 120px;
	height: 100px;
	background: #73cbd7;
	float: left;
	text-align: center;
	line-height: 100px;
}

.dept_left a{
	font-size: 26px;
	color: #FFFFFF;
}
.dept_right{
	float: left;
	width: 520px;
	height: 100px;
	background: #CCCCCC;
}
.dept_right ul{
	width: 520px;
	padding-left: 8px;
	margin-top: 10px;
}
.dept_right ul li{
	float: left;
	line-height: 40px;
	color: #000000;
	font-size: 14px;
	margin-right: 20px;
}
.dept_right ul li a:hover{
	color: #73cbd7;
}
.bottom_right{
	width: 180px;
	height: 130px;
	position: absolute;
	top: 24px;
	right: 136px;
}
.bottom_right img{
	width: 310px;
	height: 130px;
	
}
/*end 内容区域*/

/*footer*/
#footer{
	width: 100%;
	background: #000;
	clear: both;
	height: 300px;
	border-top:6px solid green;
}
#footer .footer_contact{
	width: 960px;
	margin: 0 auto;
}
.footer_contact ul{
	float: left;
	margin: 30px 60px 0 0;
}
.footer_contact ul strong{
	font-size: 14px;
	color: greenyellow;
}
.footer_contact ul strong span{
	color: #ccc;
}
.footer_contact ul li{
	font-size: 14px;
	text-indent:7px;
	padding:8px;
}
.footer_contact ul li a,.footer_statement p a{
	color:rgba(255,255,255,0.6)
}
.footer_contact ul li a:hover,.footer_statement p a:hover{
	color:rgba(255,255,255,0.8)
}

.footer_statement{
	width: 960px;
	clear: both;
	padding-top: 30px;
}
.footer_statement img{
	width: 50px;
	height: 48px;
	float: left;
	margin-right: 5px;
}
.footer_statement p {
	font-size: 12px;
	line-height: 20px;
	height: 30px;
	color:rgba(255,255,255,0.6)
}
/*end footer*/

/*centent_login*/

.centent_login{  
	width:310px;
	height:250px;
	border:1px solid rgba(200,200,200);
	background: rgba(255,255,200,0.1);
	margin-bottom: 6px;
	float:left;
}
.login_list{
	background: rgba(96,100,100,0.1);
	height: 250px;
}
.login_list:hover{
	border: 1px solid rgba(96,96,96,0.1);
}
.login_list li{
	width:310px;
}
.login_list li span,.login_list li p,.login_list li strong,.login_list li em{
	height: 50px;
	font-size: 13px;
	text-align: center;
	line-height: 50px;
}
.login_list li span{
	color: #000;
	display: block;
	font-weight: bold;
}
.login_list li p{
	color: #ccc;
}
.login_list li .regedit{
	width:180px;
	height: 40px;
	margin-left: 70px;
	background:green;
	border:none;
	border-radius:5px;
	color: #fff;
	font-size:15px;
	font-weight:bold;
}
.login_list li .regedit:hover{
	cursor:pointer;
}
.login_list li strong{
	display: block;
}
.login_list li strong a{
	color:green;
}
.login_list li strong a:hover,.login_list li em a:hover{
	text-decoration:underline;
}
.login_list li em{
	font-style:normal;
	font-size: 16px;
	padding: 2px;
}
.login_list li em a{
	color: green;
}
.login_list li .qq{
	margin-left: 85px;
}
/*end centent_login*/