/*default*/
body,h1,h2,h3,h4,h5,h6,p,span,strong,ul,li,ol{
	padding: 0;
	margin: 0;
}
body{
	font-size: 12px;
	font-family: "微软雅黑";
	background: #f1f1f1;
}
li{
	list-style: none;
}
img{
	vertical-align: top;
	border: none;
}
a{
	text-decoration: none;
	color: #000;
}
.clear{clear:both;}
/*end default*/

/*头部区域*/
.head{
	width: 100%;
	height: 80px;
	background: #3dafa7;
	position: relative;
}
.head img{
	margin: 10px 0 0 470px;
	width: 297px;
	height: 62px;
}
.head ul{
	position: absolute;
	top:0;
	right: 465px;
	width: 465px;
}
.head ul li {
	width: 150px;
	height: 80px;
	float: left;
	background: #238f87;
	margin-right: 5px;
	text-align: center;
}

.head ul li a{
	color: #FFFFFF;
	line-height: 80px;
	font-size: 18px;
}

/*中间区域*/
.contert{
	width: 960px;
	margin: 10px auto;
}
.contert_top{
	width: 960px;
}
.contert_top strong{
	font-size: 20px;
}
.contert_top span{
	font-size: 16px;
	color: #0AA6BD;
}
.contert_top img{
	width: 200px;
	height: 260px;
	margin-top: 20px;
	display: inline-block;
}
