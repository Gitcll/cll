/*container*/
.centent{
	width: 100%;
	height: 700px;
	margin: 0 auto;
	background: #87CEEB;
}
.container{
	width:335px;
	height: 500px;
	background: rgba(255,255,255);
	position: absolute;
	top:125px;
	left:800px;
}
.container ul li{
	clear:both;
	margin-left: 20px;
}
.container ul li span{
	font-size: 20px;
	font-weight: bold;
	line-height: 50px;
}
.container ul li em{
	font-size: 25px;
	float:right;
	font-style:normal;
	line-height: 40px;
}
/*.container ul li em a{
	color: #ccc;
	width: 40px;
	width: 50px;
	height: 50px;
}
.container ul li em a:hover{
	background: green;
	height: 40px;
	border-radius:40px;
}*/
.container ul li .text{
	font-size: 15px;
	color: #000;
	text-indent:4px;
}
.container ul li .btn2{
	background: green;
	border:none;
	color:#fff;
	border-radius:5px;
	font-weight: bold;
	cursor:pointer;
}
.container ul li .text_user,.container ul li .text_pass{
	width:290px;
	height: 38px;
	margin: 10px 0;
}
.container ul li .btn2{
	width:290px;
	height: 35px;
	margin: 10px 0;
}
.container ul li strong{
	height: 50px;
	font-size: 13px;
	text-align: center;
	line-height: 50px;	
	font-weight: normal;
	color: #ccc;
}
.container ul li strong a{
	color:green;
}
.container ul li strong a:hover{
	text-decoration:underline;
}
.container ul li .account{
	float:left;
}
.container ul li .disclaimer{
	float:right;
	margin-right: 25px;
}

.container ul li em{
	height: 50px;
	font-size: 14px;
	text-align: center;
	line-height: 50px;
	font-style:normal;
	padding: 5px;
	float: left;
	margin: 10px 0;
}
.container li em a{
	color: green;
}
.container li em a:hover{
	text-decoration:underline;
}
.container li .qq{
	margin-left: 85px;
}

/*end container*/
/*footer*/
#footer{
	width: 100%;
	background: #000;
	clear: both;
	height: 260px;
	border-top:6px solid green;
}
#footer .footer_contact{
	width: 960px;
	margin: 0 auto;
}
.footer_contact ul{
	float: left;
	margin: 30px 60px 0 0;
}
.footer_contact ul strong{
	font-size: 14px;
	color: greenyellow;
}
.footer_contact ul strong span{
	color: #ccc;
}
.footer_contact ul li{
	font-size: 14px;
	text-indent:7px;
	padding:6px;
}
.footer_contact ul li a,.footer_statement p a{
	color:rgba(255,255,255,0.6)
}
.footer_contact ul li a:hover,.footer_statement p a:hover{
	color:rgba(255,255,255,0.8)
}

.footer_statement{
	width: 960px;
	clear: both;
	padding-top: 10px;
}
.footer_statement img{
	width: 50px;
	height: 48px;
	float: left;
	margin-right: 5px;
}
.footer_statement p {
	font-size: 12px;
	line-height: 20px;
	height: 30px;
	
	color:rgba(255,255,255,0.6)
}
/*end footer*/
