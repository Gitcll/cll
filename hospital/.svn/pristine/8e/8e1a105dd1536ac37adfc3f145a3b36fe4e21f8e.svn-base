body,h1,h2,h3,h4,h5,h6,p,span,strong,ul,li,ol{
	padding: 0;
	margin: 0;
}
body{
	font-size: 12px;
	font-family: "微软雅黑";
	
}
li{
	list-style: none;
}
img{
	vertical-align: top;
	border: none;
}
a{
	text-decoration: none;
	color: #000;
}
